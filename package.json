{
  "name": "altair-static-slim",
  "version": "4.0.8",
  "description": "Slim version of altair-static without any dependency",
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "dist",
    "build"
  ],
  "scripts": {
    "build": "concurrently -r pnpm:types pnpm:copy:dist",
    "copy:dist": "node -r esbuild-register ./copy-dist.ts",
    "prepare": "rimraf dist build && tsup",
    "types": "dts-bundle-generator src/index.ts -o build/index.d.ts --external-inlines altair-graphql-core --external-inlines altair-static"
  },
  "devDependencies": {
    "@types/concurrently": "^6.2.1",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.4.3",
    "altair-graphql-core": "4.0.8",
    "altair-static": "4.0.8",
    "concurrently": "^6.2.0",
    "dts-bundle-generator": "^5.9.0",
    "esbuild-register": "^2.6.0",
    "fs-extra": "^10.0.0",
    "rimraf": "^3.0.2",
    "tsup": "^4.12.5",
    "typescript": "^4.3.5"
  }
}
